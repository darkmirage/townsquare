{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "module": "commonjs",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": false,
      "outDir": "dist",
      "sourceMap": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "target": "es2017"
    },
    "compileOnSave": true,
    "include": [
      "src"
    ]
}